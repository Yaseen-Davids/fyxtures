{
  "version": 2,
  "builds": [
    {
      "src": "api/src/Server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "ui/dist/**/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/src/Server.ts" },
    { "src": "/ui/(.*)", "dest": "ui/dist/$1" },
    { "src": "/(.*)", "dest": "/ui/index.html" }
  ]
}
