{
  "version": 2,
  "builds": [
    {
      "src": "/api/src/Server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "/ui/vite.config.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/src/Server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/ui/index.html"
    }
  ]
}
