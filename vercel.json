{
  "version": 2,
  "builds": [
    {
      "src": "/api/src/Server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "/ui/index.html",
      "use": "vite"
    }
  ],
  "routes": [
    {
      "src": "/api/src/(.*)",
      "dest": "api/$1"
    },
    {
      "src": "/ui/(.*)",
      "dest": "ui/$1"
    }
  ]
}
