{
  "version": 2,
  "builds": [
    {
      "src": "api/src/Server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "ui",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/src/Server.ts" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/ui/index.html" }
  ]
}
